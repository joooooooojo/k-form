{
  "name": "@joooooooooojo/kform",
  "version": "0.0.3",
  "private": false,
  "description": "二次封装element-plus的Form表单",
  "keywords": [
    "@joooooooooojo/kform",
    "element-plus",
    "typescript",
    "el-form",
    "vue3",
    "vite"
  ],
  "homepage": "https://github.com/joooooooojo/kk-form/blob/master/README.md",
  "bugs": {
    "url": "https://github.com/joooooooojo/kk-form/issues"
  },
  "license": "MIT",
  "author": "joooooooooojo",
  "repository": {
    "type": "git",
    "url": "https://github.com/joooooooojo/kk-form.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "jsdelivr": "dist/index.umd.js",
  "unpkg": "dist/index.umd.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },

  },
  "files": [
    "dist"
  ],
  "style": "dist/style.css",
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lib": "vite build && vue-tsc -p tsconfig.build.json",
    "publish": "npm pub"
  },
  "dependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^20.11.24",
    "dayjs": "^1.11.10",
    "element-plus": "^2.5.6",
    "lodash": "^4.17.21",
    "prettier": "^3.2.5",
    "vue": "^3.4.19"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vue-tsc": "^1.8.27"
  }
}
